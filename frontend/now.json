{
  "version": 2,
  "name": "dfg",
  "builds": [{"src": "package.json", "use": "@now/next"}],
  "routes": [
    {"src": "/exhibition/(.*)", "dest": "/exhibition?slug=$1"},
    {"src": "/artist/(.*)", "dest": "/artist?slug=$1"},
    {"src": "/publication/(.*)", "dest": "/publication?slug=$1"},
    {"src": "/fair/(.*)", "dest": "/fair?slug=$1"},
    {"src": "/news/(.*)", "dest": "/new?slug=$1"}
  ]
}
